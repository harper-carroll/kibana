{
    "events" : {
    "template" : "networkrules"
    "settings" : {
    "index.refresh_interval" : "10s",
     "index.cache.filter.expire" : "2m",
     "index.cache.field.expire" : "2m"
    },
    "mappings" : {
       "rule" : {
"_source" : {
"excludes" : ["App", "AppEnd", "AppID", "AppIDEnd", "CaptureKey", "FileID" ]
},
"properties" : {
"SrcIP"  : { "type": "ip", "ignore_malformed" : true },
"SrcIP6"  : { "type": "string", "index" : "not_analyzed", "ignore_malformed" : true },
"DestIP"  : { "type": "ip", "ignore_malformed" : true },
"DestIP6"  : { "type": "string", "index" : "not_analyzed", "ignore_malformed" : true },
"TimeStart" : { "format": "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||yyyy-MM-dd'T'HH:mm:ss.SSSZZ", "type": "date"},
"TimeUpdated" : { "format": "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||yyyy-MM-dd'T'HH:mm:ss.SSSZZ", "type": "date"},
"TimeTotal" : { "type": "long", "ignore_malformed" : true},
"TimeEnd" : { "type": "long", "ignore_malformed" : true},
"TimeStartRaw" : { "type": "long", "ignore_malformed" : true, "index" : "no", "store" : "no" },
"TimeUpdatedRaw" : { "type": "long", "ignore_malformed" : true, "index" : "no", "store" : "no" },
"TimeDelta" : { "type": "long", "ignore_malformed" : true },
"Captured" : { "type": "string", "null_value": "false"},
"Session" : {"type": "string", "index" : "not_analyzed"},
"SrcMAC" : {"type" : "string", "index" : "not_analyzed", "ignore_malformed" : true},
"enabled" : { "type" : "boolean" },
"severity" : { "type" : "string" },
"query" : { "type" : "object" },
"createdDate" : { "format" : "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||yyyy-MM-dd'T'HH:mm:ss.SSSZZ", "type" : "date" },
"lastModifiedDate" : { "format" : "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||yyyy-MM-dd'T'HH:mm:ss.SSSZZ", "type" : "date" }
"Date" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"SessionID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"RequestSize" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Mode" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"FileSize" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"VidDuration" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"AcctOutputOctets" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"AcctInputOctets" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"SrcNet" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"CallDuration" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"DstNet" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"FileChunkDataOffset" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgListMsgID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Index" : { "type" : "string", "index" : "not_analyzed", "store" : "yes", "ignore_malformed" : true},
"ReceiverUID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"CallWay" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"SessionID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"StartTime" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"NASIP" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"DeviceType" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"ContactUID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgListSize" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Offset" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgCode" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Code" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Login" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"FileIdentifier" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Version" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"AttachID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"CallID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"UserID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"TTL" : { "type" : "long", "store" : "yes", "index": "not_analyzed", "ignore_malformed" : true },
"StopTime" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"CommandCode" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"UID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"SenderUID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"FramedIP" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"IsMobileService" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgType" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"EndStatus" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"TID" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"Timestamp" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"MsgListDate" : { "type" : "string", "store" : "yes", "ignore_malformed" : true},
"DestMAC" : {"type" : "string", "index" : "not_analyzed", "ignore_malformed" : true}
}
}
    }
   }
}
